class Animal{
    static int x=10;
    static void show01(){
        System.out.println("Class Animal x="+x);
    }
}
class Dog extends Animal{
    static String sound="Gheu Gheu";
    static void show02(){
        System.out.println("Dog sound="+sound+" 10");
    }
    public static void main(String[] args) {
        show01();
        show02();

    }
}